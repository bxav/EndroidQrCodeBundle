{
    "name": "endroid/qrcode-bundle",
    "description": "Endroid Qr Code Bundle for Symfony",
    "keywords": ["endroid", "qrcode", "qr", "code", "symfony", "bundle"],
    "homepage": "https://github.com/endroid/EndroidQrCodeBundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "endroid/qrcode": "~1.3",
        "symfony/http-kernel": "~2.3",
        "sensio/framework-extra-bundle": "~3.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.9",
        "phpunit/phpunit": "~4.7",
        "symfony/browser-kit": "~2.7",
        "symfony/framework-bundle": "~2.7",
        "symfony/var-dumper": "~2.7"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Bundle\\QrCodeBundle\\": [ "src/", "tests/" ]
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
